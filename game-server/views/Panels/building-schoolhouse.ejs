            <h5>In Ausbildung</h5>
            <%= runningQueue %>
        <% } %>

        <h5>Wird ausgebildet</h5>
        <% queue.forEach((type) => { %>
            <%= type %><br/>
        <% }) %>

        <h5>Ausbilden</h5>
        <button data-ui-action="createCharacter" data-type="serf">Tr√§ger</button>
        <button data-ui-action="createCharacter" data-type="laborer">Bauarbeiter</button>
